{
  "name": "repdev",
  "description": "Embed Roblox's new Creator Dashboard into the old /develop page",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.roblox.com/develop"],
      "js": ["roblox.js"]
    }
  ]
}
